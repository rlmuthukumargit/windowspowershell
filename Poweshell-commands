Get-CimInstance -ClassName Win32_LogicalDisk -ComputerName "172.31.95.234" -Filter "DriveType=3" | Select-Object DeviceID, @{Name="Size(GB)";Expression={"{0:N2}" -f ($_.Size/1GB)}}, @{Name="FreeSpace(GB)";Expression={"{0:N2}" -f ($_.FreeSpace/1GB)}}




